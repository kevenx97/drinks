{
  "extends": "airbnb",
  "rules": {
    "no-plusplus": "off",
    "react/prop-types": "off",
    "import/no-unresolved": "off",
    "no-use-before-define": "off",
    "class-methods-use-this": "off",
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off",
    "semi": [
      "error",
      "never"
    ],
    "linebreak-style": "off",
    "global-require": "off"
  },
  "env": {
    "jest": true
  }
}